<Window x:Name="MainWindow1" x:Class="ImageServiceGUI.View.MainWindow"
        xmlns:Views="clr-namespace:ImageServiceGUI.Views"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ImageServiceGUI.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="459" Width="616">
    <TabControl x:Name="MyTabControl"  HorizontalAlignment="Left" Height="420" VerticalAlignment="Top" Width="600" Foreground="{x:Null}" BorderBrush="{x:Null}" Background="{x:Null}">
        <TabItem x:Name="SettingsTab" Header="Settings">
            <Views:SettingsView Margin="0,0,-6,0" Foreground="Black"/>
        </TabItem>
        <TabItem x:Name="LogTab" Header="Log">
            <Views:LogView>
                
            </Views:LogView>
        </TabItem>
    </TabControl>
</Window>
